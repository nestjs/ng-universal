{
  "name": "@nestjs/ng-universal",
  "version": "4.2.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@ng-universal)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:lib && npm run build:schematics",
    "build:lib": "tsc -p tsconfig.json",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@angular/common": "^10.0.0 || ^11.0.0",
    "@angular/platform-server": "^10.0.0 || ^11.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nguniversal/common": "^10.0.0 || ^11.0.0",
    "@nguniversal/express-engine": "^10.0.0 || ^11.0.0",
    "express": "^4.17.1",
    "zone.js": "^0.10.0 || ^0.11.0"
  },
  "devDependencies": {
    "@angular/common": "11.0.3",
    "@angular/platform-server": "11.0.3",
    "@nestjs/common": "7.5.5",
    "@nestjs/core": "7.5.5",
    "@types/memory-cache": "0.2.1",
    "@types/node": "13.13.5",
    "express": "4.17.1",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-morph": "9.1.0",
    "typescript": "4.1.2",
    "zone.js": "0.11.3"
  },
  "dependencies": {
    "@angular-devkit/schematics": "11.0.3",
    "@schematics/angular": "11.0.3",
    "@nguniversal/common": "11.0.1",
    "@nguniversal/express-engine": "11.0.1",
    "memory-cache": "0.2.0"
  },
  "schematics": "./schematics/collection.json",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
