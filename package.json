{
  "name": "@nestjs/ng-universal",
  "version": "3.1.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@ng-universal)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:lib && npm run build:schematics",
    "build:lib": "tsc -p tsconfig.json",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@angular/common": "^9.0.0",
    "@angular/platform-server": "^9.0.0",
    "@nestjs/common": "^6.0.0 || ^7.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0",
    "@nguniversal/common": "^9.0.0",
    "@nguniversal/express-engine": "^9.0.0",
    "express": "^4.17.1",
    "zone.js": "^0.8.29 || ^0.9.0 || ^0.10.0"
  },
  "devDependencies": {
    "@angular/common": "9.0.7",
    "@angular/platform-server": "9.0.7",
    "@nestjs/common": "7.0.5",
    "@nestjs/core": "7.0.5",
    "@types/memory-cache": "0.2.1",
    "@types/node": "13.9.3",
    "express": "4.17.1",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "2.0.2",
    "ts-morph": "7.0.0",
    "typescript": "3.8.3",
    "zone.js": "0.10.3"
  },
  "dependencies": {
    "@angular-devkit/schematics": "9.0.6",
    "@schematics/angular": "9.0.6",
    "@nguniversal/common": "9.0.2",
    "@nguniversal/express-engine": "9.0.2",
    "memory-cache": "0.2.0"
  },
  "schematics": "./schematics/collection.json",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
